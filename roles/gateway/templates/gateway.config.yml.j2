http:
  port: 8080
admin:
  port: 9876
  hostname: localhost
apiEndpoints:
  api:
    host: localhost
    paths: '/hello'
serviceEndpoints:
  serviceA:
    url: 'http://10.0.10.21:8080'
policies:
  - basic-auth
  - cors
  - expression
  - key-auth
  - log
  - oauth2
  - proxy
  - rate-limit
pipelines:
  basic:
    apiEndpoints:
      - api
    policies:
      - proxy:
          - action:
              serviceEndpoint: serviceA
              changeOrigin: false
